#include "STD_TYPES.h"
#include "DEFINES.h"

#include "DIO_interface.h"
#include "PORT_interface.h"
#include "GIE_interface.h"
#include "KERNEL_interface.h"

#include <util/delay.h>

void Main_voidLed1(void);
void Main_voidLed2(void);

/*It is the main*/
void main(void)
{
	PORT_voidInit();

	RTOS_u8CreateTask(0, 1000, Main_voidLed1);
	RTOS_u8CreateTask(1, 1000, Main_voidLed2);
	RTOS_u8SuspendTask(0);
	RTOS_u8ResumeTask(0);

	GIE_voidEnable();
	RTOS_voidStart();

	while(1)
	{
	}
}

void Main_voidLed1(void)
{
	DIO_u8TogglePinValue(DIO_u8PORTC, DIO_u8PIN0);
}
void Main_voidLed2(void)
{
	DIO_u8TogglePinValue(DIO_u8PORTC, DIO_u8PIN1);
}
