#include "STD_TYPES.h"
#include "DEFINES.h"
#include "BIT_MATH.h"

#include "DIO_interface.h"
#include "PORT_interface.h"
#include "UART_interface.h"
#include "GIE_interface.h
"
#include "MOTOR_interface.h"

#include <util/delay.h>

/*It is the main*/
void main(void)
{
	gie_
	USART_voidInit();

	MOTOR_t Local_stMotor1;
	Local_stMotor1.Pin1 = DIO_u8PIN0;
	Local_stMotor1.Pin2 = DIO_u8PIN1;
	Local_stMotor1.Port = DIO_u8PORTC;

	MOTOR_t Local_stMotor2;
	Local_stMotor2.Pin1 = DIO_u8PIN2;
	Local_stMotor2.Pin2 = DIO_u8PIN3;
	Local_stMotor2.Port = DIO_u8PORTC;

	while(1)
	{
		MOTOR_u8RotateCW(&Local_stMotor1);
		MOTOR_u8RotateCW(&Local_stMotor2);

	}
}
